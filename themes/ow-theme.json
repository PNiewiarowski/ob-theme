    "tokenColors": [
      {
        "name": "#777777",
        "scope": [
          "comment"
        ],
        "settings": {
          "foreground": "#777777",
          "fontStyle": ""
        }
      },
      {
        "name": "#f8dcba",
        "scope": [
          "constant.other.placeholder",
          "entity.name.import",
          "meta.template.expression",
        ],
        "settings": {
          "foreground": "#f8dcba",
          "fontStyle": "bold"
        }
      },
      {
        "name": "#e67e22",
        "scope": [
          "keyword",
          "support.type",
          "storage.type",
          "variable.language.this",
          "entity.name.tag"
        ],
        "settings": {
          "foreground": "#e67e22",
          "fontStyle": ""
        }
      },
      {
        "name": "#f1c40f",
        "scope": [
          "support.function",
          "entity.name.function",
          "variable.other.property",
        ]
          ,
        "settings": {
          "foreground": "#f1c40f",
          "fontStyle": ""
        }
      },
      {
        "name": "#ccf4dd",
        "scope": [
          "string"
        ],
        "settings": {
          "foreground": "#94c900",
          "fontStyle": ""
        }
      },
      {
        "name": "#f377f3",
        "scope": [
          "constant",
        ],
        "settings": {
          "foreground": "#f377f3",
          "fontStyle": ""
        }
      },
      {
        "name": "#ffbd6c",
        "scope": [
          "entity.name.import.go",
          "variable",
          "storage.modifier",
          "entity.other.attribute-name",
        ],
        "settings": {
          "foreground": "#ffbd6c",
        }
